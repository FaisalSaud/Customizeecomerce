<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.abu.customizeecomerce.ChooseIconImage">

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="@string/ChooseIcon"
        android:textColor="@color/cardview_dark_background"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/icon1"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:clickable="true"
        app:srcCompat="@drawable/food_icon2"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="76dp"
        android:layout_marginTop="40dp"
        android:layout_below="@+id/icon5"
        android:layout_alignLeft="@+id/icon5"
        android:layout_alignStart="@+id/icon5" />

    <ImageView
        android:id="@+id/icon2"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:clickable="true"
        app:srcCompat="@drawable/bookshelf_icon"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="76dp"
        android:layout_alignTop="@+id/icon7"
        android:layout_alignLeft="@+id/icon4"
        android:layout_alignStart="@+id/icon4" />" />

    <ImageView
        android:id="@+id/icon3"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:clickable="true"
        app:srcCompat="@drawable/car_icon"
        tools:layout_editor_absoluteX="270dp"
        tools:layout_editor_absoluteY="76dp"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="29dp"
        android:layout_alignTop="@+id/icon7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:id="@+id/icon4"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:clickable="true"
        app:srcCompat="@drawable/computer_icon"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="198dp"
        android:layout_marginRight="25dp"
        android:layout_marginEnd="25dp"
        android:layout_alignTop="@+id/icon6"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:id="@+id/icon5"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:clickable="true"
        app:srcCompat="@drawable/flower_icon"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="198dp"
        android:layout_alignTop="@+id/icon6"
        android:layout_alignLeft="@+id/icon3"
        android:layout_alignStart="@+id/icon3" />

    <ImageView
        android:id="@+id/icon6"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginTop="56dp"
        android:clickable="true"
        app:srcCompat="@drawable/gamecontroller_icon"
        tools:layout_editor_absoluteX="270dp"
        tools:layout_editor_absoluteY="198dp"
        android:layout_below="@+id/icon7"
        android:layout_alignLeft="@+id/icon7"
        android:layout_alignStart="@+id/icon7" />

    <ImageView
        android:id="@+id/icon7"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:clickable="true"
        app:srcCompat="@drawable/paintcan_icon"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="322dp"
        android:layout_marginTop="105dp"
        android:layout_below="@+id/textView18"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
